{
	"hosting": {
		"public": "dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/academic/**",
				"destination": "/academic/index.html"
			},

			{
				"source": "/sign-in",
				"destination": "/index.html"
			},
			{
				"source": "/sign-up",
				"destination": "/index.html"
			},
			{
				"source": "/landing",
				"destination": "/index.html"
			},
			{
				"source": "/*",
				"destination": "/index.html"
			}
		]
	}
}